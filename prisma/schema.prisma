generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("PRISMA_DATABASE_URL")
}

model binance_market_data {
  symbol             String   @db.VarChar(20)
  date               DateTime @db.Date
  open               Float
  high               Float
  low                Float
  close              Float
  volume             Float
  close_time         DateTime @db.Date
  quote_volume       Float
  trades             Int
  taker_base_volume  Float
  taker_quote_volume Float

  @@id([symbol, date])
}
